<!-- views/eateries/show.ejs -->
<!DOCTYPE html>
<html>
<head>
  <title>Where can I get lunch around here?</title>
  <link rel="stylesheet" href="https://unpkg.com/@picocss/pico@1.*/css/pico.classless.min.css" />
</head>
<body>
  <main>
    <nav>
      <ul>
        <li><a href="/">&lt; Back</a></li>
      </ul>
    </nav>

    <h1><%= eatery.name %></h1>
    <h2><strong><%= eatery.genre %></strong> - <small><%= eatery.price %></small></h2>
    <h6><%= eatery.address %></h6>

    <iframe
      src="<%= `https://maps.google.com/maps?output=embed&q=${encodeURIComponent(eatery.address)}` %>"
      width="500"
      height="400"
    ></iframe>
  </main>
</body>
</html>
